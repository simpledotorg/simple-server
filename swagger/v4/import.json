        "name": {
          "type": [
            "object",
            "null"
          ],
          "properties": {
            "text": {
              "type": [
                "string",
                "null"
              ]
            }
          },
          "description": "Full name of the patient. Client can send anonymised names.                           If name is unset, Simple will generate a random name.",
          "required": [
            "text"
          ]
        },
        "telecom": {
          "type": [
            "null",
            "array"
          ],
          "items": {
            "$ref": "#/definitions/contact_point"
