        "gender": {
          "type": "string",
          "nullable": false,
          "enum": [
            "male",
            "female",
            "other"
          ],
          "description": "FHIR does not have a code for transgender, but Simple does.                    To accomodate this use case, we are considering 'other' to mean transgender."
        },
        "birthDate": {
          "type": "string",
          "format": "date",
          "description": "Date of birth. Can be an approximation."
        },
        "deceasedBoolean": {
          "type": [
            "boolean",
            "null"
          ]
        },
        "managingOrganization": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "nullable": false,
                "description": "Currently assigned facility ID"
              }
