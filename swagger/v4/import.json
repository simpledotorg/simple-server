          "maxItems": 1,
          "items": {
            "type": [
              "object",
              "null"
            ],
            "properties": {
              "line": {
                "type": [
                  "null",
                  "array"
                ],
                "maxItems": 1,
                "items": {
                  "type": "string",
                  "description": "street address"
