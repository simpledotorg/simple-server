            "createdAt"
          ]
        },
        "identifier": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "nullable": false,
                "description": "ID of the patient in the partner org database"
              }
            },
            "required": [
              "value"
            ]
          },
          "nullable": false,
          "minItems": 1,
          "maxItems": 1
        },
        "active": {
          "type": "boolean",
          "description": "Status of the patient. Will be ignored and marked as dead if deceasedBoolean is true",
          "default": true
