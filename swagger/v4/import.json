        "meta": {
          "type": "object",
          "properties": {
            "lastUpdated": {
              "type": "string",
              "format": "date-time",
              "description": "Timestamp with millisecond precision."
            },
            "createdAt": {
              "type": "string",
              "format": "date-time",
              "description": "Timestamp with millisecond precision."
            }
          },
          "nullable": false,
