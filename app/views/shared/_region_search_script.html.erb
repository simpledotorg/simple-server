<script>
  const search = new RegionsSearch().search;
  const form = $("#search-form");

  form.on("submit", function(event) {
    input = $(event.target).find(".typeahead-input");
    search(input);
    return false;
  });

  form.on("keyup", function(event) {
    search(event.target);
    return false;
  });
</script>