<div class="progress">
  <div class="progress-bar bg-success" role="progressbar" style="width: <%= progress[:migrated] || 0 %>%" aria-valuenow="<%= progress[:migrated] || 0 %>" aria-valuemin="0" aria-valuemax="100"></div>
  <div class="progress-bar bg-danger" role="progressbar" style="width: <%= progress[:errors] || 0 %>%" aria-valuenow="<%= progress[:errors] || 0 %>" aria-valuemin="0" aria-valuemax="100"></div>
</div>
